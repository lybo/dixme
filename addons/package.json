{
  "name": "addons",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "autoprefixer": "7.1.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "chokidar": "1.7.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "express": "4.15.3",
    "file-loader": "0.11.2",
    "find": "0.2.7",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "react": "^16.5.2",
    "react-dev-utils": "^3.0.2",
    "react-dom": "^16.5.2",
    "react-scripts-webextension": "^0.8.5",
    "style-loader": "0.18.2",
    "tree-from-paths": "^1.0.0",
    "url-loader": "0.5.9",
    "web-ext-types": "^2.2.0",
    "webpack": "3.4.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "addons-start-js": "node addons/scripts/start.js",
    "addons-start": "npm-run-all -p watch-css addons-start-js",
    "addons-build-js": "node addons/scripts/build.js",
    "addons-build": "npm-run-all build-css addons-build-js",
    "addons-test": "node addons/scripts/test.js --env=jsdom"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
